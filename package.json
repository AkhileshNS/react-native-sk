{
  "name": "react-native-sk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "clean": "npm run clean:android && npm run clean:ios",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "postversion": "react-native-version",
    "test": "jest --watchAll",
    "lint": "eslint ."
  },
  "dependencies": {
    "elegant-standard": "^2.0.0",
    "lodash": "^4.17.15",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-exception-handler": "^2.10.8",
    "react-native-vector-icons": "^6.6.0",
    "react-regex-router": "^1.0.0",
    "react-spring": "^8.0.27",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/runtime": "7.5.5",
    "@react-native-community/eslint-config": "0.0.3",
    "babel": "^6.23.0",
    "babel-jest": "24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "6.1.0",
    "jest": "24.8.0",
    "jest-environment-enzyme": "^7.1.0",
    "jest-enzyme": "^7.1.0",
    "metro-react-native-babel-preset": "0.54.1",
    "react-dom": "^16.9.0",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "jest-enzyme",
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "moduleNameMapper": {
      "styled-components": "<rootDir>/node_modules/styled-components/native/dist/styled-components.native.cjs.js"
    }
  },
  "babel": {
    "plugins": [
      "transform-inline-environment-variables",
      [
        "module-resolver",
        {
          "root": [
            "."
          ],
          "alias": {
            "src": "./src"
          }
        }
      ]
    ]
  }
}
